<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.omair.migrateto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Posts"></activity>
        <activity
            android:name=".profile_info"
            android:label="@string/title_activity_profile_info"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".login_screen"
            android:label="@string/title_activity_login_screen"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".resources_local"
            android:label="@string/title_activity_resources_local"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SpeechTranslation"
            android:label="@string/title_activity_speech_translation"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>